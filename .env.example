# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Document AI Configuration
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id
GOOGLE_CLOUD_LOCATION=us
GOOGLE_CLOUD_STORAGE_BUCKET=your_storage_bucket_name

# For local development (use file path)
GOOGLE_APPLICATION_CREDENTIALS=./credentials/google-service-account.json

# For Vercel deployment (use base64 encoded service account JSON)
# Generate with: base64 -i ./credentials/google-service-account.json
# GOOGLE_APPLICATION_CREDENTIALS_BASE64=your_base64_encoded_service_account

# Document AI Processors
GOOGLE_CLOUD_PROCESSOR_ID=your_primary_processor_id
GOOGLE_CLOUD_FORM_PARSER_ID=your_form_parser_id
GOOGLE_CLOUD_OCR_PROCESSOR_ID=your_ocr_processor_id

# Pinecone Configuration
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_INDEX_NAME=pdf-searcher

# Cron Job Security
CRON_SECRET=your_secure_random_string

# Optional: Upstash Redis for Vercel Caching
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# Optional: Local Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password